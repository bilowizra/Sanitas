<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.doctor_profile">

    <EditText
        android:id="@+id/genderValueEdit"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="290dp"
        android:layout_marginEnd="92dp"
        android:layout_toEndOf="@+id/docGenderTextView"
        android:layout_weight="10"
        android:editable="true"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="N/A"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/ageValueEdit"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="false"
        android:layout_marginStart="10dp"
        android:layout_marginTop="390dp"
        android:layout_marginEnd="116dp"
        android:layout_toEndOf="@+id/docAgeTextView"
        android:layout_weight="10"
        android:editable="true"
        android:inputType="number"
        android:minHeight="48dp"
        android:text="@string/n_a"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/docAgeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="400dp"
        android:text="Age:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/specialtyValueEdit"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="false"
        android:layout_marginStart="10dp"
        android:layout_marginTop="340dp"
        android:layout_marginEnd="72dp"
        android:layout_toEndOf="@+id/docSpeTextView"
        android:layout_weight="10"
        android:editable="true"
        android:inputType="text"
        android:minHeight="48dp"
        android:text="@string/n_a"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/phoneValueEdit"
        android:layout_width="211dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/docPhoneTextView"
        android:layout_weight="10"
        android:editable="true"
        android:inputType="phone"
        android:minHeight="48dp"
        android:text="@string/n_a"
        android:textColor="@color/black"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/resTC"
        android:layout_width="224dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="200dp"
        android:layout_toEndOf="@+id/docTCTextView"
        android:text="@string/tc"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/docSpeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="350dp"
        android:text="@string/specialty"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/docGenderTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="300dp"
        android:text="@string/gender"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/docPhoneTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="250dp"
        android:text="@string/phone_number"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/docTCTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="200dp"
        android:text="@string/tc_id3"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="46dp"
        android:text="@string/personal_information"
        android:textColor="@color/red"
        android:textSize="34sp"
        android:textStyle="bold"
        tools:ignore="RelativeOverlap" />

    <TextView
        android:id="@+id/docNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="150dp"
        android:text="@string/name"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/resName"
        android:layout_width="224dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="150dp"
        android:layout_toEndOf="@+id/docNameTextView"
        android:text="@string/name5"
        android:textSize="20sp" />

    <Button
        android:id="@+id/editDoctorProfileButton"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_marginTop="550dp"
        android:layout_marginStart="110dp"
        android:text="@string/edit_profile"
        android:textSize="20sp" />


</RelativeLayout>